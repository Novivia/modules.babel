{
  "name": "@novivia/babel",
  "role": "babel",
  "version": "1.0.0",
  "engines": {
    "node": ">=4",
    "npm": ">=3",
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "publishConfig": {
    "registry": "https://npm.novivia.com"
  },
  "description": "Novivia Node.js Babel configurations and utilities.",
  "authors": [
    "Mathieu M-Gosselin <mathieumg@novivia.com>",
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git.novivia.com/nov/modules.babel"
  },
  "main": "index.js",
  "novivia-builder": {
    "babelPatterns": [
      "config/**/*.js",
      "ignoreRegex.js",
    ],
  },
  "novivia-linter": {
    "targets": {
      "global": ["config", "lib"],
    },
  },
  "novivia-tester": {
    "coverageLocations": [
      "!lib/localRequireHook.js",
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
      },
    },
  },
  "scripts": {
    "build": "novivia-builder build",
    "lint": "novivia-linter lint",
    "open-source": "open-source",
    "pub": "build-module publish",
    "release": "build-module release",
    "test": "novivia-tester",
  },
  "devDependencies": {
    // Novivia dependencies.
    "@novivia/build-module": "^0.5.2",
    "@novivia/linter": "^1.10.1",
    "@novivia/open-sourcer": "^0.1.2",
    "@novivia/tester": "^1.0.0",

    // npm dependencies.
    "babel-preset-es2015":"^6.22.0",
    "common-tags":"^1.4.0",
  },
  "dependencies": {
    // npm dependencies.
    "babel-plugin-add-module-exports":"^0.2.1",
    "babel-plugin-flow-react-proptypes":"^0.21.0",
    "babel-plugin-minify-dead-code-elimination":"^0.1.3",
    "babel-plugin-minify-replace":"0.0.3",
    "babel-plugin-react-transform":"^2.0.2",
    "babel-plugin-transform-decorators-legacy":"^1.3.4",
    "babel-plugin-transform-flow-comments":"^6.22.0",
    "babel-plugin-transform-runtime":"^6.23.0",
    "babel-plugin-typecheck":"^3.9.0",
    "babel-polyfill":"^6.23.0",
    "babel-preset-env":"^1.1.8",
    "babel-preset-react":"^6.23.0",
    "babel-preset-stage-0":"^6.22.0",
    "babel-register":"^6.23.0",
    "json5":"^0.5.1",
    "pkginfo-json5":"^0.5.0",
    "react-transform-hmr": "^1.0.4",
    "semver":"^5.3.0",
  },
}
